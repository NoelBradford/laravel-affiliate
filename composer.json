{
    "name": "noelbradford/laravel-affiliate",
    "description": "A Affiliate System With Laravel",
    "keywords": ["referral", "referral-system", "affiliate-tracking"],
    "license": "MIT",
    "authors": [{
        "name": "noelbradford",
        "email": "contact@noelbradford.com"
    }],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "laravel/laravel": "^5.5",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "noelbradford\\Referral\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "noelbradford\\Referral\\ReferralServiceProvider"
            ]
        }
    }
}
